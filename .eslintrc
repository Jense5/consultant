{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "flowtype"
  ],
  "rules": {
    "import/no-extraneous-dependencies": [
      "error", {
        "devDependencies": true
      }
    ],
    "no-new": "off",
    "import/no-dynamic-require": "off",
    "global-require": "off"
  },
  "globals": {
    "describe": true,
    "it": true
  }
}
