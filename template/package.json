{
  "name": "{{ name }}",
  "version": "{{ version }}",
  "description": "{{ description }}",
  "main": "lib/index.js",
  "repository": "{{ repository }}",
  "author": "{{ author }}",
  "license": "{{ license }}",
  "keywords": [
    "{{ name }}"
  ],
  "main": {
    "{{ command }}": "./lib/index.js"
  },
  "scripts": {
    "clear": "rimraf lib dist",
    "lint": "eslint src test",
    "start": "./lib/index.js",
    "test": "mocha --compilers js:babel-core/register",
    "build": "babel src -d lib && chmod +x lib/index.js",
    "prepublish": "npm run lint && npm run test && npm run clear && npm run build"
  },
  "dependencies": {
    "chalk": "^1.1.3",{{#if commander}}
    "commander": "^2.9.0",{{/if}}{{#if lodash}}
    "lodash": "^4.17.4",{{/if}}{{#if cheerio}}
    "cheerio": "^0.22.0",
    "request": "^2.79.0",{{/if}}
    "winston": "^2.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-core": "^6.21.0",
    "babel-eslint": "^7.1.1",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babel-preset-env": "^1.1.8",
    "chai": "^3.5.0",
    "eslint": "^3.13.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "flow-bin": "^0.37.4",
    "mocha": "^3.2.0",
    "rimraf": "^2.5.4"
  }
}
